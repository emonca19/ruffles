{
  "name": "react_tests",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:unit": "vitest run src/service/ src/utils/",
    "test:functional": "vitest run src",
    "test:login": "vitest run src/service/autentication.test.jsx",
    "test:registro": "vitest run  src/service/RegisterForm.test.jsx",
    "test:registro:watch": "vitest src/service/RegisterForm.test.jsx",
    "test:registro:coverage": "vitest run src/service/RegisterForm.test.jsx --coverage",
    "test:sorteo": "vitest run src/service/crearSorteo.test.jsx",
    "test:sorteo:watch": "vitest src/service/crearSorteo.test.jsx",
    "test:sorteo:coverage": "vitest run src/service/crearSorteo.test.jsx --coverage",
    "test:tabla-numeros": "vitest run src/service/TablaNumeros.test.jsx --reporter=verbose",
    "test:tabla-numeros:report": "vitest run src/service/tablaNumeros.test.jsx --reporter=junit --outputFile=./test-results/tabla-numeros.xml",
    "test:panel_personal":"vitest run src/service/panel_personal.test.jsx",
    "test:panel_personal_interface":"vitest run src/service/panel_personal_interface.test",
    "test:prueba_componente":"vitest run src/service/prueba_componente_seleccion.test.jsx",
    "test:prueba_renderizado_admin":"vitest run src/service/renderizado_admin.test.jsx",
    "test:prueba_visor_comprobante" : "vitest run src/service/visor_comprobante.test.jsx",
    "test:prueba_raffles":"vitest run src/service/raffles.test.jsx",
    "test:all": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^27.1.0",
    "vite": "^7.1.7",
    "vitest": "^4.0.8"
  }
}
